{% extends "base.html.twig" %}

{% block title %}{{ parent() }} - {{ 'visitor information'|trans }} {% endblock %}

{% block body %}
    <div class="container">
        <div class="progress">
            <div class="progress-bar progress-bar-info" role="progressbar" aria-valuenow="75"
                 aria-valuemin="0" aria-valuemax="100" style="width:100%">
                {{ 'step' |trans }} 4/4
            </div>
        </div>
        <div class="jumbotron jumbotron-fluid">
            <h2 class="display-3">{{ 'admission is free for children under 4 years old !'|trans|capitalize }}</h2>
            <hr class="my-4">

        </div>

        </div>

{% endblock %}
